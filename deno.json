{
  "tasks": {
    "run": "deno run --import-map=import_map.json --allow-env --allow-net --allow-write=. --allow-read=. src/index.ts",
    "dev": "deno run --inspect-brk --import-map=import_map.json --allow-env --allow-net --allow-write=. --allow-read=. src/index.ts",
    "check": "deno check --import-map=import_map.json src/index.ts && deno check --import-map=import_map.json src/action.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "nano-jsx"
  }
}
